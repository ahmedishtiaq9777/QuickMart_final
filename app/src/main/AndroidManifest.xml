<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.demotxt.myapp.recyclerview">
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.INTERNET"/>



    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity android:name=".activity.MainActivity2">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".fragment.MapFragment"></activity>
        <activity android:name=".activity.Book_Activity"></activity>

        <activity android:name=".fragment.UsersettingFragment"></activity>


        <activity android:name=".activity.Signup"></activity>
        <activity android:name=".shoppyorders.ShoppyOrderActivity"></activity>
        <activity android:name=".activity.Prod_Activity"></activity>
        <activity android:name=".activity.r3_Activity"></activity>
        <activity android:name=".activity.Login"></activity>

        <activity android:name=".Test"></activity>
        <activity android:name=".productlist.ShoppyProductListActivity"></activity>





    </application>

</manifest>